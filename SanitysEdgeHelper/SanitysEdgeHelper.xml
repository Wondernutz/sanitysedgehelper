<GuiXml>
  <Controls>
    <TopLevelControl name="SEHMessage1" mouseEnabled="true" movable="true" clampedToScreen="true">
      <Dimensions x="800" y="200" />
      <Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="CENTER" offsetY="-150" />
      <OnMoveStop>SEH.OnSEHMessage1Move()</OnMoveStop>

      <Controls>
        <Label name="$(parent)Label" width="800" height="200" font="EsoUI/Common/Fonts/univers67.otf|72|soft-shadow-thick" inheritAlpha="true" color="FF6600"
            wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER" text="FIREBOMBS: |cB2E80B15.5s|r [0.5%]">
          <Anchor point="TOP" relativeTo="$(parent)" relativePoint="TOP" />
        </Label>
      </Controls>
    </TopLevelControl>

    <TopLevelControl name="SEHMessage2" mouseEnabled="true" movable="true" clampedToScreen="true">
      <Dimensions x="800" y="200" />
      <Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="CENTER" offsetY="50" />
      <OnMoveStop>SEH.OnSEHMessage2Move()</OnMoveStop>
      
      <Controls>
        <Label name="$(parent)Label" width="800" height="200" font="EsoUI/Common/Fonts/univers67.otf|72|soft-shadow-thick" inheritAlpha="true" color="FF5733"
            wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER" text="MESSAGE TWO" alpha="0.85">
          <Anchor point="TOP" relativeTo="$(parent)" relativePoint="TOP" />
        </Label>
      </Controls>
    </TopLevelControl>

    <TopLevelControl name="SEHMessage3" mouseEnabled="true" movable="true" clampedToScreen="true">
      <Dimensions x="800" y="200" />
      <Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="CENTER" offsetY="150" />
      <OnMoveStop>SEH.OnSEHMessage3Move()</OnMoveStop>
      
      <Controls>
        <Label name="$(parent)Label" width="800" height="200" font="EsoUI/Common/Fonts/univers67.otf|72|soft-shadow-thick" inheritAlpha="true" color="FF00FF"
            wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER" text="MESSAGE THREE" alpha="0.85">
          <Anchor point="TOP" relativeTo="$(parent)" relativePoint="TOP" />
        </Label>
      </Controls>
    </TopLevelControl>

    <TopLevelControl name="SEHStatus" mouseEnabled="true" movable="true" clampedToScreen="true">
      <Dimensions x="500" y="200" />
      <Anchor point="BOTTOM" relativeTo="GuiRoot" relativePoint="CENTER" offsetY="-150" offsetX="600" />
      <OnMoveStop>SEH.OnSEHStatusMove()</OnMoveStop>
      <Controls>
      <Label name="$(parent)LabelAddonName" font="EsoUI/Common/Fonts/univers67.otf|16|soft-shadow-thick" color="FFFFFF" text="Sanity's Edge Helper" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="-30"/>
      </Label>
      
      <Texture name="$(parent)Divider" textureFile="/esoui/art/miscellaneous/horizontaldivider.dds">
          <Dimensions x="270" y="4"/>
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="-20" offsetX="-210"/>
      </Texture>
      
      <Label name="$(parent)LabelTop" font="ZoFontWinH1" color="FFFFFF" text="TOP MESSAGE" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="-60" offsetX="0"/>
      </Label>

      <!-- Boss 1: Yaseyla -->
      <Label name="$(parent)LabelYaseyla1" font="ZoFontWinH1" color="CCCC66" text="Next Shrapnel:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="0"/>
      </Label>
      <Label name="$(parent)LabelYaseyla1Value" font="ZoFontWinH1" color="ff8500" text="60s" verticalAlignment="CENTER" horizontalAlignment="LEFT" alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="0" offsetX="230"/>
      </Label>

      <Label name="$(parent)LabelYaseyla2" font="ZoFontWinH1" color="FF6600" text="Next Firebombs:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="40"/>
      </Label>
      <Label name="$(parent)LabelYaseyla2Value" font="ZoFontWinH1" color="ff8500" text="INC" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="40" offsetX="230"/>
      </Label>

      <Label name="$(parent)LabelYaseyla3" font="ZoFontWinH1" color="66CCFF" text="Next Frostbombs:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="80"/>
      </Label>
      <Label name="$(parent)LabelYaseyla3Value" font="ZoFontWinH1" color="ff8500" text="INC" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="80" offsetX="230"/>
      </Label>

      <Label name="$(parent)LabelYaseyla4" font="ZoFontWinH1" color="A8A9AD" text="Next Chain:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="120"/>
      </Label>
      <Label name="$(parent)LabelYaseyla4Value" font="ZoFontWinH1" color="ff8500" text="INC" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="120" offsetX="230"/>
      </Label>

      <!-- Boss 2: Chimera -->
      <Label name="$(parent)LabelChimera1" font="ZoFontWinH1" color="CCCC66" text="Chimera Despawn:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="0"/>
      </Label>
      <Label name="$(parent)LabelChimera1Value" font="ZoFontWinH1" color="ff8500" text="-" verticalAlignment="CENTER" horizontalAlignment="LEFT" alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="0" offsetX="250"/>
      </Label>

      <Label name="$(parent)LabelChimera2" font="ZoFontWinH1" color="FFD666" text="Chain Lightning:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="40"/>
      </Label>
      <Label name="$(parent)LabelChimera2Value" font="ZoFontWinH1" color="ff8500" text="INC" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="40" offsetX="250"/>
      </Label>

      <!-- Boss 3: Ansuul -->
      <Label name="$(parent)LabelAnsuul1" font="ZoFontWinH1" color="A8A9AD" text="Next Calamity:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="0"/>
      </Label>
      <Label name="$(parent)LabelAnsuul1Value" font="ZoFontWinH1" color="ff8500" text="-" verticalAlignment="CENTER" horizontalAlignment="LEFT" alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="0" offsetX="250"/>
      </Label>

      <Label name="$(parent)LabelAnsuul2" font="ZoFontWinH1" color="303B4C" text="Next:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="40"/>
      </Label>
      <Label name="$(parent)LabelAnsuul2Value" font="ZoFontWinH1" color="ff8500" text="INC" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="40" offsetX="250"/>
      </Label>

      <Label name="$(parent)LabelAnsuul3" font="ZoFontWinH1" color="FF0000" text="Red Boss:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="80"/>
      </Label>
      <Label name="$(parent)LabelAnsuul3Value" font="ZoFontWinH1" color="ff8500" text="100.0%" verticalAlignment="CENTER" horizontalAlignment="LEFT" alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="80" offsetX="250"/>
      </Label>

      <Label name="$(parent)LabelAnsuul4" font="ZoFontWinH1" color="00FF00" text="Green Boss:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="120"/>
      </Label>
      <Label name="$(parent)LabelAnsuul4Value" font="ZoFontWinH1" color="ff8500" text="100.0%" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="120" offsetX="250"/>
      </Label>

      <Label name="$(parent)LabelAnsuul5" font="ZoFontWinH1" color="0000FF" text="Blue Boss:" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="160"/>
      </Label>
      <Label name="$(parent)LabelAnsuul5Value" font="ZoFontWinH1" color="ff8500" text="100.0%" verticalAlignment="CENTER" horizontalAlignment="LEFT"  alpha="0.85">
          <Dimensions x="600" y="50" />
          <Anchor point="CENTER"  relativeTo="$(parent)" relativePoint="CENTER" offsetY="160" offsetX="250"/>
      </Label>

      </Controls>
    </TopLevelControl>

    <TopLevelControl name="SEHScreenBorder" tier="HIGH" alpha="0.5" hidden="true">
      <AnchorFill />
      <Controls>
        <Backdrop name="$(parent)Overlay" edgeColor="CC00CC">
          <AnchorFill />
          <Center file="EsoUI/Art/HUD/UITelvarOverlayCenter.dds" />
          <Edge file="EsoUI/Art/HUD/UITelvarOverlayEdge.dds" edgeFileWidth="2048" edgeFileHeight="256" />
          <Insets left="256" top="256" right="-256" bottom="-256" />
        </Backdrop>
      </Controls>
    </TopLevelControl>
  </Controls>
</GuiXml>
